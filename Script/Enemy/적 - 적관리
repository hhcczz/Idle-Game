            new Monster("최상급 악마 <color=#FF7474>XIV</color>",  31700000, 31700000,  2740000,  8400000,  7100,  13.5f,  31),
            new Monster("최상급 악마 <color=#FF7474>XV</color>",   31800000, 31800000,  2750000,  8500000,  7200,  13.5f,  31),
            new Monster("최상급 악마 <color=#FF7474>XVI</color>",  31900000, 31900000,  2760000,  8600000,  7300,  13.5f,  31),
            new Monster("최상급 악마 <color=#FF7474>XVII</color>", 32000000, 32000000,  2770000,  8700000,  7400,  13.5f,  31),
            new Monster("최상급 악마 <color=#FF7474>BOSS</color>", 40000000, 40000000,  3000000,  9000000,  7500,  13.5f,  31),
        };

        // 다른 몬스터들을 몬스터 리스트에 추가
        monsters.AddRange(slimes);
    }

    

