    {
        // 딕셔너리 초기화
        GameManager.stageClearDict["EasySlime"] = new bool[18];
        GameManager.stageClearDict["NormalSlime"] = new bool[18];
        GameManager.stageClearDict["HardSlime"] = new bool[18];
        GameManager.stageClearDict["ExtremeSlime"] = new bool[18];

